---
layout: layouts/tags
permalink: /tags/
eleventyNavigation:
  parent: root
  key: tags
  title: Tags
  order: 2
  icon: /src/_includes/svg/tags.svg

    
---

<main class="flex flex-wrap pt-24 px-4 mx-auto overflow-hidden shadow sm:w-10/12 md:w-10/12 lg:w-8/12">
  <div class="w-full prose dark:prose-invert md:prose-lg lg:prose-xl">
<h1>Tags</h1>

{% for tag in collections.tags %}
  {% set tagUrl %}/tags/{{ tag }}/{% endset %}
  <a href="{{ tagUrl | url }}">
      <span class="tag tag-sm tag-{{ tag }}">
        #{{ tag }}
      </span>
  </a>
  
{% endfor %}

</div>
</main>


 

{
    "@context": "https://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [
    {%- for entry in $navPages -%}
    {%- set absolutePostUrl = entry.url | url | absoluteUrl(metadata.url) %}
    {{ itemListElement(loop.index, entry.title, absolutePostUrl) }}
    {%- if not loop.last -%},{%- endif -%}
    {%- endfor -%}
    ]
}
{
  "name": "jmsalazardev-blog",
  "version": "1.1.0",
  "homepage": "https://github.com/jmsalazardev/jmsalazardev-blog#readme",
  "description": "Blog de desarrollo de software",
  "scripts": {
    "start": "cross-env NODE_ENV=development run-p serve watch:* --silent",
    "clean": "npx rimraf public/",
    "build": "cross-env NODE_ENV=production run-s clean build:*",
    "build:js": "rollup --config",
    "build:css": "npx postcss src/_assets/css/global.css --o src/_includes/assets/bundle.css",
    "build:eleventy": "npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "deploy": "firebase deploy",
    "watch:js": "npm run build:js -- --watch --no-watch.clearScreen",
    "watch:css": "npm run build:css -- --watch"
  },
  "author": {
    "name": "Jos√© Miguel Salazar",
    "email": "admin@jmsalazar.dev",
    "url": "https://jmsalazar.dev"
  },
  "license": "MIT",
